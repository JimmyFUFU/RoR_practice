<!DOCTYPE html>
<html>
  <head>
    <title>SnapaskPractice</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% flash.each do |key, value| %>
        <div id="flashword">
        <%= value %>
        </div>
    <% end %>
    <%= yield %>
  </body>
</html>
