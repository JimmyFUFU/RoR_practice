<h1>成為管理者</h1>

<%= form_for(@manager) do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %> <br />

  <%= f.label :email, "Email" %>
  <%= f.email_field :email %> <br />

  <%= f.label :password, "Password" %>
  <%= f.password_field :password %> <br />

  <%= f.submit "Sign Up"%>
<% end %>

<br />
